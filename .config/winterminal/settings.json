{
    "$schema": "https://aka.ms/terminal-profiles-schema",
    "actions": 
    [
        {
            "command": 
            {
                "action": "splitPane",
                "split": "vertical"
            },
            "keys": "alt+shift+plus"
        },
        {
            "command": 
            {
                "action": "splitPane",
                "index": 1,
                "split": "vertical"
            },
            "keys": "alt+2"
        },
        {
            "command": 
            {
                "action": "copy",
                "singleLine": false
            },
            "keys": "ctrl+c"
        },
        {
            "command": "paste",
            "keys": "ctrl+v"
        },
        {
            "command": 
            {
                "action": "splitPane",
                "split": "horizontal"
            },
            "keys": "alt+shift+minus"
        },
        {
            "command": 
            {
                "action": "newTab",
                "index": 1
            },
            "keys": "ctrl+2"
        },
        {
            "command": 
            {
                "action": "splitPane",
                "split": "auto"
            },
            "keys": "alt+shift+1"
        },
        {
            "command": "find",
            "keys": "ctrl+shift+f"
        },
        {
            "command": 
            {
                "action": "splitPane",
                "split": "auto",
                "splitMode": "duplicate"
            },
            "keys": "alt+shift+d"
        },
        {
            "command": 
            {
                "action": "splitPane",
                "index": 0,
                "split": "vertical"
            },
            "keys": "alt+1"
        },
        {
            "command": 
            {
                "action": "splitPane",
                "index": 2,
                "split": "vertical"
            },
            "keys": "alt+3"
        },
        {
            "command": 
            {
                "action": "newTab",
                "index": 0
            },
            "keys": "ctrl+1"
        },
        {
            "command": 
            {
                "action": "newTab",
                "index": 2
            },
            "keys": "ctrl+3"
        },
        {
            "command": "closePane",
            "keys": "alt+q"
        }
    ],
    "copyFormatting": "none",
    "copyOnSelect": true,
